<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="no.larseknu.hiof.bottomnavigationplay.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="no.larseknu.hiof.bottomnavigationplay.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_collections"
        android:name="no.hiof.larseknu.playingwithbottomnavigation.collectionscreen.CollectionFragment"
        android:label="@string/title_collections"
        tools:layout="@layout/fragment_collections" />
</navigation>